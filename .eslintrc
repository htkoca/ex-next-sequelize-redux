{
  "extends": ["airbnb", "prettier", "plugin:import"],
  "plugins": ["prettier"],
  "rules": {
    "react/prop-types": 0,
    "react/react-in-jsx-scope": "off",
    "prettier/prettier": ["error"],
    "no-console": 0,
    "no-unused-vars": 0,
    "lines-between-class-members": 0,
    "no-useless-escape": 0,
    "class-methods-use-this": 0,
    "jsx-a11y/anchor-is-valid": 0
  },
  "globals": {
    "React": "writable"
  },
  "settings": {
    "import/resolver": {
      "alias": {
        "map": [["@", "./"]],
        "extensions": [".ts", ".js", ".jsx", ".json"]
      }
    }
  }
}
